<div class="sidebar">
  
  <div class="sidebar_title_box">
    <h3 class="sidebar_title">Online Users</h3>
  </div>


  <div class="online_users_list">

    <% @users.each do |user| %>
      <% next if user == current_user %>
      <div class="online_user" id="<%= user.id %>">
        <%= user.username %>
      </div>
    <% end %>
   
  </div>

</div>
